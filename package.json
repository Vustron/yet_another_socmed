{
  "name": "yet_another_socmed",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "bunx @biomejs/biome lint --write --unsafe src",
    "format": "bunx @biomejs/biome format --write src",
    "fmt:fix": "bun run lint && bun run format && bun run typecheck",
    "check-dep": "bunx upgradeps -v",
    "prod-test": "turbo build && turbo start",
    "db-generate": "bunx prisma generate",
    "db-push": "bunx prisma db push --schema ./src/prisma/schema.prisma"
  },
